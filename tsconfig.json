{
  "compilerOptions": {
    "target": "es5",
		"lib": ["es6", "dom"],
		"types": ["reflect-metadata", "jest"],
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
    "module": "commonjs",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
